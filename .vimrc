set nocompatible
set autochdir
set history=2000
filetype on
filetype indent on
filetype plugin on
filetype plugin indent on


set nobackup
set noswapfile
set autoread

set cursorline

" set number
set nowrap

set list
set listchars=tab:>-,trail:-

set smartindent
set tabstop=4
set shiftwidth=4
set expandtab
set softtabstop=4
set smarttab
